<!DOCTYPE digester-rules PUBLIC          
          "-//Apache Commons //DTD digester-rules XML V1.0//EN"           
          "http://commons.apache.org/digester/dtds/digester-rules-3.0.dtd">

<digester-rules>

<pattern value="*/bulkOperationMetaData">
	<object-create-rule
		classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationMetadata" />
	<set-properties-rule />
</pattern>

<pattern value="*/bulkOperationClass">
	<object-create-rule
		classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass" />
	<set-properties-rule />
	<set-next-rule methodname="addBulkOperationClass" />
</pattern>

   <pattern value="*/BulkOperationMetadata">
    <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationMetadata"/>
	<set-properties-rule/>	
  </pattern>
  <pattern value="*/BulkOperationClass">
    <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass"/>
	<set-properties-rule/>	
	<set-next-rule methodname="addBulkOperationClass"/>
  </pattern>
  
  <pattern value="*/containmentAssociation">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addContainmentAssociation"/>
  </pattern>
  
  <pattern value="*/referenceAssociation">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addReferenceAssociation"/>
  </pattern>
  
  <pattern value="*/attribute">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.Attribute"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addAttribute"/>	  
  </pattern>
  
  <pattern value="*/containmentAssociationCollection">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addContainmentAssociation"/>
  </pattern>
  
  <pattern value="*/attributeCollection">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.Attribute"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addAttribute"/>	  
  </pattern>
   <pattern value="*/referenceAssociationCollection">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addReferenceAssociation"/>
  </pattern>
    
   <pattern value="*/hookingInformation">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.HookingInformation"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="setHookingInformation"/>	  
  </pattern>
  
  <pattern value="*/discriminatorCollection">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.AttributeDiscriminator"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addAttributeDiscriminator"/>	  
  </pattern>
  
  <pattern value="*/dynExtCategoryAssociationCollection">
      <object-create-rule classname="com.krishagni.catissueplus.bulkoperator.metadata.BulkOperationClass"/>
	  <set-properties-rule/>
	  <set-next-rule methodname="addDynExtCategoryAssociation"/>	  
  </pattern>
  
</digester-rules>