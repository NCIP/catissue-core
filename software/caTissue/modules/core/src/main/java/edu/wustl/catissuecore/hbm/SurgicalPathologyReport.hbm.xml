<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="edu.wustl.catissuecore.domain.pathology.SurgicalPathologyReport"
		table="CATISSUE_PATHOLOGY_REPORT" dynamic-update="false" dynamic-insert="false" lazy="true">

		<cache usage="read-write" />

		<id name="id" column="IDENTIFIER" type="long" length="30" unsaved-value="null">
			<generator class="native">
				<param name="sequence">CATISSUE_PATHOLOGY_REPORT_SEQ</param>
			</generator>
		</id>

		<property name="activityStatus" type="string" update="true" insert="true"
			column="ACTIVITY_STATUS" length="100" />

		<one-to-one name="binaryContent" class="edu.wustl.catissuecore.domain.pathology.BinaryContent"
			cascade="save-update" constrained="false" property-ref="surgicalPathologyReport" />

		<property name="isFlagForReview" type="java.lang.Boolean" update="true" insert="true"
			column="REVIEW_FLAG" length="10" not-null="false" />

		<set name="pathologyReportReviewParameterCollection" table="CATISSUE_REVIEW_PARAMS"
			lazy="true" inverse="false" cascade="all" sort="unsorted">

			<cache usage="read-write" />

			<key column="REPORT_ID" />

			<one-to-many class="edu.wustl.catissuecore.domain.pathology.PathologyReportReviewParameter" />
		</set>

		<many-to-one name="reportSource" class="edu.wustl.catissuecore.domain.Site"
			cascade="none" update="true" insert="true" column="SOURCE_ID" not-null="false" />

		<one-to-one name="textContent" class="edu.wustl.catissuecore.domain.pathology.TextContent"
			cascade="save-update" constrained="false" property-ref="surgicalPathologyReport" />

		<one-to-one name="xmlContent" class="edu.wustl.catissuecore.domain.pathology.XMLContent"
			cascade="save-update" constrained="false" property-ref="surgicalPathologyReport" />

		<property name="reportStatus" type="string" update="true" insert="true"
			column="REPORT_STATUS" length="100" />

		<property name="collectionDateTime" type="date" update="true" insert="true"
			column="COLLECTION_DATE_TIME" />

		<many-to-one name="specimenCollectionGroup" class="edu.wustl.catissuecore.domain.SpecimenCollectionGroup"
			cascade="none" update="true" insert="true" column="SCG_ID" not-null="false" />

		<!--
			To add non XDoclet property mappings, create a file named
			hibernate-properties-SurgicalPathologyReport.xml containing the additional properties
			and place it in your merge dir.
		-->

		<joined-subclass
			name="edu.wustl.catissuecore.domain.pathology.DeidentifiedSurgicalPathologyReport"
			table="CATISSUE_DEIDENTIFIED_REPORT" dynamic-update="false" dynamic-insert="false"
			lazy="true">
			<key column="IDENTIFIER" />

			<set name="conceptReferentCollection" table="CATISSUE_CONCEPT_REFERENT" fetch="join"
				lazy="false" inverse="false" cascade="all" sort="unsorted">

				<cache usage="read-write" />

				<key column="DEIDENTIFIED_REPORT_ID" />

				<one-to-many class="edu.wustl.catissuecore.domain.pathology.ConceptReferent" />
			</set>

			<property name="isQuarantined" type="string" update="true" insert="true"
				column="STATUS" />

			<set name="quarantineEventParameterCollection" table="CATISSUE_QUARANTINE_PARAMS"
				fetch="join" lazy="false" inverse="false" cascade="none" sort="unsorted">

				<cache usage="read-write" />

				<key column="DEID_REPORT_ID" />

				<one-to-many class="edu.wustl.catissuecore.domain.pathology.QuarantineEventParameter" />
			</set>

		</joined-subclass>
		<joined-subclass
			name="edu.wustl.catissuecore.domain.pathology.IdentifiedSurgicalPathologyReport" table="CATISSUE_IDENTIFIED_REPORT"
			dynamic-update="false" dynamic-insert="false" lazy="true">
			<key column="IDENTIFIER" />

			<many-to-one name="deIdentifiedSurgicalPathologyReport"
				class="edu.wustl.catissuecore.domain.pathology.DeidentifiedSurgicalPathologyReport"
				cascade="none" update="true" insert="true" column="DEID_REPORT" not-null="false" />

		</joined-subclass>

	</class>

</hibernate-mapping>
