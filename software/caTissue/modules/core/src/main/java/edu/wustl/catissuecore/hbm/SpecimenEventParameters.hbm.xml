<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.SpecimenEventParameters"
        table="CATISSUE_SPECIMEN_EVENT_PARAM"
		lazy="true"
    >

    <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPEC_EVENT_PARAM_SEQ</param>
              <!--
                  To add non XDoclet generator parameters, create a file named
                  hibernate-generator-params-SpecimenEventParameters.xml
                  containing the additional parameters and place it in your merge dir.
              -->
            </generator>
        </id>
   <many-to-one
            name="specimenCollectionGroup"
            class="edu.wustl.catissuecore.domain.SpecimenCollectionGroup"
            cascade="none"

            update="true"
            insert="true"
            column="SPECIMEN_COLL_GRP_ID"
        />
        <many-to-one
            name="specimen"
            class="edu.wustl.catissuecore.domain.AbstractSpecimen"
            cascade="none"
			lazy="false"
            update="true"
            insert="true"
            column="SPECIMEN_ID"
        />

        <property
            name="timestamp"
            type="timestamp"
            update="true"
            insert="true"
            column="EVENT_TIMESTAMP"
        />

        <many-to-one
            name="user"
            class="edu.wustl.catissuecore.domain.User"
            cascade="none"

            update="true"
            insert="true"
            column="USER_ID"
        />

        <property
            name="comment"
            type="string"
            update="true"
            insert="true"
            column="COMMENTS"
            length="500"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SpecimenEventParameters.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="edu.wustl.catissuecore.domain.FixedEventParameters"
            table="CATISSUE_FIXED_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="fixationType"
            type="string"
            update="true"
            insert="true"
            column="FIXATION_TYPE"
            length="50"
            not-null="true"
        />

        <property
            name="durationInMinutes"
            type="int"
            update="true"
            insert="true"
            column="DURATION_IN_MINUTES"
            length="30"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.FrozenEventParameters"
            table="CATISSUE_FROZEN_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="method"
            type="string"
            update="true"
            insert="true"
            column="METHOD"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.CheckInCheckOutEventParameter"
            table="CATISSUE_IN_OUT_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="storageStatus"
            type="string"
            update="true"
            insert="true"
            column="STORAGE_STATUS"
            length="100"
            not-null="true"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.ProcedureEventParameters"
            table="CATISSUE_PROCEDURE_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="url"
            type="string"
            update="true"
            insert="true"
            column="URL"
            length="255"
            not-null="true"
        />

        <property
            name="name"
            type="string"
            update="true"
            insert="true"
            column="NAME"
            length="255"
            not-null="true"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.SpunEventParameters"
            table="CATISSUE_SPUN_EVENT_PARAMETERS"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="gravityForce"
            type="double"
            update="true"
            insert="true"
            column="GFORCE"
            length="30"
        />

        <property
            name="durationInMinutes"
            type="int"
            update="true"
            insert="true"
            column="DURATION_IN_MINUTES"
            length="30"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.TransferEventParameters"
            table="CATISSUE_TRANSFER_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="fromPositionDimensionOne"
            type="int"
            update="true"
            insert="true"
            column="FROM_POSITION_DIMENSION_ONE"
            length="30"
        />

        <property
            name="fromPositionDimensionTwo"
            type="int"
            update="true"
            insert="true"
            column="FROM_POSITION_DIMENSION_TWO"
            length="30"
        />

        <property
            name="toPositionDimensionOne"
            type="int"
            update="true"
            insert="true"
            column="TO_POSITION_DIMENSION_ONE"
            length="30"
        />

        <property
            name="toPositionDimensionTwo"
            type="int"
            update="true"
            insert="true"
            column="TO_POSITION_DIMENSION_TWO"
            length="30"
        />

        <many-to-one
            name="toStorageContainer"
            class="edu.wustl.catissuecore.domain.StorageContainer"
            cascade="none"

            update="true"
            insert="true"
            column="TO_STORAGE_CONTAINER_ID"
        />

        <many-to-one
            name="fromStorageContainer"
            class="edu.wustl.catissuecore.domain.StorageContainer"
            cascade="none"

            update="true"
            insert="true"
            column="FROM_STORAGE_CONTAINER_ID"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.CollectionEventParameters"
            table="CATISSUE_COLL_EVENT_PARAM"
			lazy="false"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="collectionProcedure"
            type="string"
            update="true"
            insert="true"
            column="COLLECTION_PROCEDURE"
            length="50"
        />

        <property
            name="container"
            type="string"
            update="true"
            insert="true"
            column="CONTAINER"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.ReceivedEventParameters"
            table="CATISSUE_RECEIVED_EVENT_PARAM"
			lazy="false"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="receivedQuality"
            type="string"
            update="true"
            insert="true"
            column="RECEIVED_QUALITY"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.EmbeddedEventParameters"
            table="CATISSUE_EMBEDDED_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="embeddingMedium"
            type="string"
            update="true"
            insert="true"
            column="EMBEDDING_MEDIUM"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.ThawEventParameters"
            table="CATISSUE_THAW_EVENT_PARAMETERS"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.ReviewEventParameters"
            table="CATISSUE_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />

        <joined-subclass
            name="edu.wustl.catissuecore.domain.CellSpecimenReviewParameters"
            table="CATISSUE_CELL_SPE_REVIEW_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="neoplasticCellularityPercentage"
            type="double"
            update="true"
            insert="true"
            column="NEOPLASTIC_CELLULARITY_PER"
            length="30"
        />

        <property
            name="viableCellPercentage"
            type="double"
            update="true"
            insert="true"
            column="VIABLE_CELL_PERCENTAGE"
            length="30"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.TissueSpecimenReviewEventParameters"
            table="CATISSUE_TIS_SPE_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="neoplasticCellularityPercentage"
            type="double"
            update="true"
            insert="true"
            column="NEOPLASTIC_CELLULARITY_PER"
            length="30"
        />

        <property
            name="necrosisPercentage"
            type="double"
            update="true"
            insert="true"
            column="NECROSIS_PERCENTAGE"
            length="30"
        />

        <property
            name="lymphocyticPercentage"
            type="double"
            update="true"
            insert="true"
            column="LYMPHOCYTIC_PERCENTAGE"
            length="30"
        />

        <property
            name="totalCellularityPercentage"
            type="double"
            update="true"
            insert="true"
            column="TOTAL_CELLULARITY_PERCENTAGE"
            length="30"
        />

        <property
            name="histologicalQuality"
            type="string"
            update="true"
            insert="true"
            column="HISTOLOGICAL_QUALITY"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.FluidSpecimenReviewEventParameters"
            table="CATISSUE_FLUID_SPE_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="cellCount"
            type="double"
            update="true"
            insert="true"
            column="CELL_COUNT"
            length="30"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.MolecularSpecimenReviewParameters"
            table="CATISSUE_MOL_SPE_REVIEW_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="gelImageURL"
            type="string"
            update="true"
            insert="true"
            column="GEL_IMAGE_URL"
            length="255"
        />

        <property
            name="qualityIndex"
            type="string"
            update="true"
            insert="true"
            column="QUALITY_INDEX"
            length="50"
        />

        <property
            name="laneNumber"
            type="string"
            update="true"
            insert="true"
            column="LANE_NUMBER"
            length="50"
        />

        <property
            name="gelNumber"
            type="int"
            update="true"
            insert="true"
            column="GEL_NUMBER"
            length="30"
        />

        <property
            name="absorbanceAt260"
            type="double"
            update="true"
            insert="true"
            column="ABSORBANCE_AT_260"
            length="30"
        />

        <property
            name="absorbanceAt280"
            type="double"
            update="true"
            insert="true"
            column="ABSORBANCE_AT_280"
            length="30"
        />

        <property
            name="ratio28STo18S"
            type="double"
            update="true"
            insert="true"
            column="RATIO_28S_TO_18S"
            length="30"
        />

        </joined-subclass>

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.DisposalEventParameters"
            table="CATISSUE_DISPOSAL_EVENT_PARAM"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="reason"
            type="string"
            update="true"
            insert="true"
            column="REASON"
            length="255"
         />

        </joined-subclass>

    </class>

</hibernate-mapping>
