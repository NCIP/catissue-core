<?xml version="1.0" encoding="UTF-8" ?>
<ServiceGroupRegistrationParameters xmlns:sgc="http://mds.globus.org/servicegroup/client"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing"
  xmlns:agg="http://mds.globus.org/aggregator/types"
  xmlns="http://mds.globus.org/servicegroup/client">
  <!-- The ServiceGroupEPR defines the servicegroup to which registrations will be made -->
  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>REGISTRATION_REFRESH</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns5="http://domain.catissuecore.wustl.edu/Catissue_cacore/types"
			xmlns:ns4="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain.pathology.ws"
			xmlns:ns14="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.service"
			xmlns:ns21="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.dataservice"
			xmlns:ns1="gme://caCORE.caCORE/3.2/edu.wustl.common.domain.ws"
			xmlns:ns22="http://org.cagrid.dataservice.cql/QueryLanguageSupport"
			xmlns:ns16="http://CQL.caBIG/2/org.cagrid.cql2"
			xmlns:ns11="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain"
			xmlns:ns9="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain.pathology"
			xmlns:ns12="gme://caCORE.caCORE/3.2/edu.wustl.common.domain"
			xmlns:ns20="http://domain.catissuecore.wustl.edu/Catissue_cacore/CQLResultTypes"
			xmlns:ns7="gme://caCORE.caCORE/3.2/edu.common.dynamicextensions.domain.integration"
			xmlns:ns10="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain.shippingtracking"
			xmlns:ns6="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns23="http://gov.nih.nci.cagrid.data/DataServiceExceptions"
			xmlns:ns13="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"
			xmlns:ns15="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common"
			xmlns:ns19="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLResultSet"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns0="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain.ws"
			xmlns:ns3="gme://caCORE.caCORE/3.2/edu.common.dynamicextensions.domain.integration.ws"
			xmlns:ns17="http://CQL.caBIG/2/org.cagrid.cql2.results"
			xmlns:ns2="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain.deintegration.ws"
			xmlns:ns18="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery"
			xmlns:ns8="gme://caCORE.caCORE/3.2/edu.wustl.catissuecore.domain.deintegration">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>INDEX_REFRESH</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
		<agg:ResourcePropertyNames>ns21:DomainModel</agg:ResourcePropertyNames>
	
		<agg:ResourcePropertyNames>ns13:ServiceMetadata</agg:ResourcePropertyNames>
	
		<agg:ResourcePropertyNames>ns22:QueryLanguageSupport</agg:ResourcePropertyNames>
	
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
