<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.DistributedItem"
        table="CATISSUE_DISTRIBUTED_ITEM"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_DISTRIBUTED_ITEM_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-DistributedItem.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="quantity"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="30"
        />

        <many-to-one
            name="specimen"
            class="edu.wustl.catissuecore.domain.Specimen"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_ID"
        />
        
       <!-- Added by Ashish --> 
        <many-to-one
            name="specimenArray"
            class="edu.wustl.catissuecore.domain.SpecimenArray"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_ARRAY_ID"
        />
		<!-- END -->
        <many-to-one
            name="distribution"
            class="edu.wustl.catissuecore.domain.Distribution"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="DISTRIBUTION_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-DistributedItem.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
