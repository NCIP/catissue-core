<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.Specimen"
        table="CATISSUE_SPECIMEN"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPECIMEN_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Specimen.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <discriminator
            column="SPECIMEN_CLASS"
        />
	
	 <set
            name="consentTierStatusCollection"
            table="CATISSUE_CONSENT_TIER_STATUS"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.ConsentTierStatus"
              />
        </set>

        <property
            name="type"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
        />

        <property
            name="available"
            type="boolean"
            update="true"
            insert="true"
            column="AVAILABLE"
        />

        <property
            name="positionDimensionOne"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_ONE"
            length="30"
        />

        <property
            name="positionDimensionTwo"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_TWO"
            length="50"
        />

        <property
            name="barcode"
            type="string"
            update="true"
            insert="true"
            column="BARCODE"
            length="255"
            unique="true"
        />

        <property
            name="comment"
            type="string"
            update="true"
            insert="true"
            column="COMMENTS"
            length="2000"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="50"
        />

        <many-to-one
            name="parentSpecimen"
            class="edu.wustl.catissuecore.domain.Specimen"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARENT_SPECIMEN_ID"
        />

        <set
            name="biohazardCollection"
            table="CATISSUE_SPECIMEN_BIOHZ_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="SPECIMEN_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.Biohazard"
                column="BIOHAZARD_ID"
                outer-join="auto"
             />

        </set>

        <set
            name="specimenEventCollection"
            table="CATISSUE_SPECIMEN_EVENT"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="SPECIMEN_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.SpecimenEventParameters"
            />

        </set>

        <set
            name="childrenSpecimen"
            table="CATISSUE_SPECIMEN"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="PARENT_SPECIMEN_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.Specimen"
            />

        </set>

        <many-to-one
            name="storageContainer"
            class="edu.wustl.catissuecore.domain.StorageContainer"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="STORAGE_CONTAINER_IDENTIFIER"
        />

        <set
            name="externalIdentifierCollection"
            table="CATISSUE_EXTERNAL_IDENTIFIER"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="SPECIMEN_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.ExternalIdentifier"
            />

        </set>

        <many-to-one
            name="specimenCollectionGroup"
            class="edu.wustl.catissuecore.domain.SpecimenCollectionGroup"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_COLLECTION_GROUP_ID"
        />

        <many-to-one
            name="specimenCharacteristics"
            class="edu.wustl.catissuecore.domain.SpecimenCharacteristics"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_CHARACTERISTICS_ID"
        />

        <component
            name="availableQuantity"
            class="edu.wustl.catissuecore.domain.Quantity"
        >

        <property
            name="value"
            type="double"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY"
            length="50"
        />

        </component>

        <component
            name="initialquantity"
            class="edu.wustl.catissuecore.domain.Quantity"
        >

        <property
            name="value"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

        </component>

        <property
            name="pathologicalStatus"
            type="string"
            update="true"
            insert="true"
            column="PATHOLOGICAL_STATUS"
            length="50"
        />

        <property
            name="label"
            type="string"
            update="true"
            insert="true"
            column="LABEL"
            length="255"
        />

        <property
            name="lineage"
            type="string"
            update="true"
            insert="true"
            column="LINEAGE"
            length="50"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Specimen.xml
            containing the additional properties and place it in your merge dir.
        -->
        <subclass
            name="edu.wustl.catissuecore.domain.FluidSpecimen"
            discriminator-value="Fluid"
        >

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-FluidSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.CellSpecimen"
            discriminator-value="Cell"
        >

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-CellSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.MolecularSpecimen"
            discriminator-value="Molecular"
        >
        <property
            name="concentrationInMicrogramPerMicroliter"
            type="double"
            update="true"
            insert="true"
            column="CONCENTRATION"
            length="50"
        />

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-MolecularSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.TissueSpecimen"
            discriminator-value="Tissue"
        >

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-TissueSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>

    </class>

</hibernate-mapping>