<ScrubberConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ScrubberConfiguration.xsd">
	<ScrubberImplementation>TEXT</ScrubberImplementation>
	<TextProcessingRules>
		<SavedIdentifiers>
			<Label>Names</Label>
			<FileLocation>names.list</FileLocation>
			<ReplaceWith>xxxNAMExxx</ReplaceWith>
			<CaseSensitive>false</CaseSensitive>
		</SavedIdentifiers>
		<RegularExpressions>
			<FileLocation>regex.list</FileLocation>
			<ReplaceWith>xxxx</ReplaceWith>
			<PrintRegexNameInResult>true</PrintRegexNameInResult>
		</RegularExpressions>
	</TextProcessingRules>
	<Recorder>
		<Implementation>DEFAULT</Implementation>
		<EnableRecording>true</EnableRecording>
		<ConfigParam key="Text" value="Text"/>
	</Recorder>
</ScrubberConfiguration>
