<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class 
    name="com.krishagni.rbac.domain.GroupRole" 
    table="RBAC_GROUP_ROLES">

    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">RBAC_GROUP_ROLES_SEQ</param>
      </generator>
    </id>

    <many-to-one
      name="group"
      class="com.krishagni.rbac.domain.Group"
      cascade="none"
      column="GROUP_ID"/>
          
    <many-to-one
      name="role"
      class="com.krishagni.rbac.domain.Role"
      cascade="none"
      column="ROLE_ID"/>
    
    <property name="dsoId" column="DSO_ID"/>
  </class> 
</hibernate-mapping>