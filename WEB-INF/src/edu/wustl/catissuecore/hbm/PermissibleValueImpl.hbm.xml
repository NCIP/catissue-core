<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.common.cde.PermissibleValueImpl"
        table="CATISSUE_PERMISSIBLE_VALUE"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_PERMISSIBLE_VALUE_SEQ</param>
            </generator>
        </id>

        <property
            name="conceptid"
            type="string"
            update="true"
            insert="true"
            column="CONCEPT_CODE"
            length="20"
        />

        <property
            name="defination"
            type="string"
            update="true"
            insert="true"
            column="DEFINITION"
            length="500"
        />

        <many-to-one
            name="parentPermissibleValue"
            class="edu.wustl.common.cde.PermissibleValueImpl"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARENT_IDENTIFIER"
        />

        <set
            name="subPermissibleValues"
            table="CATISSUE_PERMISSIBLE_VALUE"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="PARENT_IDENTIFIER"
              />

              <one-to-many
                  class="edu.wustl.common.cde.PermissibleValueImpl"
              />
        </set>

        <property
            name="value"
            type="string"
            update="true"
            insert="true"
            column="VALUE"
            length="100"
        />

        <many-to-one
            name="cde"
            class="edu.wustl.common.cde.CDEImpl"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PUBLIC_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PermissibleValueImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
