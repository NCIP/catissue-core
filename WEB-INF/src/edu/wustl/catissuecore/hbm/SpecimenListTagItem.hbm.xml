<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="edu.wustl.common.tags.domain.TagItem" entity-name="SpecimenListTagItem" table="catissue_spec_tag_items">
		<id name="identifier" type="java.lang.Long" column="identifier">
			<generator class="increment" />
		</id>
		<many-to-one entity-name="SpecimenListTag" name="tag">
			<column name="TAG_ID" not-null="true" />
		</many-to-one>

		<property name="objId" type="java.lang.Long" column="OBJ_ID"/>
	</class>
</hibernate-mapping>
