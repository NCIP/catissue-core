<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.ClinicalStudyEventEntry"
        table="CATISSUE_CLIN_STUDY_EVNT_NTRY"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_STUDY_EVENT_ENTRY_SEQ</param>
            </generator>
        </id>

        <property
            name="entryNumber"
            type="int"
            update="true"
            insert="true"
            column="ENTRY_NUMBER"
            length="10"
        />

        <many-to-one
            name="clinicalStudyEvent"
            class="edu.wustl.catissuecore.domain.ClinicalStudyEvent"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="CLINICAL_STUDY_EVENT_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ClinicalStudyEventEntry.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
