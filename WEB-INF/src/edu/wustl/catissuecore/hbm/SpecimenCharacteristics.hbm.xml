<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.SpecimenCharacteristics"
        table="CATISSUE_SPECIMEN_CHAR"
		lazy="false" 
    >
    
    <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPECIMEN_CHAR_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SpecimenCharacteristics.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="tissueSite"
            type="string"
            update="true"
            insert="true"
            column="TISSUE_SITE"
            length="150"
        />

        <property
            name="tissueSide"
            type="string"
            update="true"
            insert="true"
            column="TISSUE_SIDE"
            length="50"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SpecimenCharacteristics.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
