<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.pathology.ReportLoaderQueue"
        table="CATISSUE_REPORT_QUEUE"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_REPORT_QUEUE_SEQ</param>
            </generator>
        </id>

        <property
            name="status"
            type="string"
            update="true"
            insert="true"
            column="STATUS"
            length="10"
        />

        <set
            name="participantCollection"
            table="CATISSUE_REPORT_PARTICIP_REL"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="REPORT_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.Participant"
                  column="PARTICIPANT_ID"
                  outer-join="auto"
              />

        </set>

        <property
            name="reportText"
            type="string"
            update="true"
            insert="true"
            column="REPORT_TEXT"
            length="4000"
        />

        <many-to-one
            name="specimenCollectionGroup"
            class="edu.wustl.catissuecore.domain.SpecimenCollectionGroup"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_COLL_GRP_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ReportLoaderQueue.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
