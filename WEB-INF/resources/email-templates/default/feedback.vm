
<table cellspacing="0" cellpadding="0" border="0" width="100%" style="border-spacing:0;border-collapse:separate">
  <tbody>
    <tr>
      <td style="border-collapse:collapse;">
        <table cellspacing="0" cellpadding="0" border="0" width="100%" style="border-collapse:collapse;font-family:Arial,sans-serif;font-size:14px;line-height:20px">
          <tbody>
            <tr>
              <td style="padding:0px;border-collapse:collapse;padding:0px;padding-bottom:10px">
                <p style="margin:10px 0 0 0">
                  Hello,
                  <br><br>
                  A new feedback is submitted with following details:
                  <br><br>
                </p>
              </td>
            </tr>
            <tr>
              <td>
                <table>
                  <tr>
                    <td>Last Name </td> <td> : $user.lastName</td>
                  </tr>
                  <tr>
                    <td>First Name </td><td> : $user.firstName </td>
                  </tr>
                  <tr>
                    <td>Login Name </td> <td>: $user.loginName </td>
                  </tr>
                  <tr>
                    <td>Email Address </td><td> : $user.emailAddress </td>
                  </tr>
                  <tr>
                    <td>AppUrl </td> <td> : $appUrl </td>
                  </tr>
                  <tr>
                    <td>Subject </td> <td> : $feedback.subject </td>
                  </tr>
                  <tr>
                    <td>Feedback </td> <td> : $feedback.feedback </td>
                  </tr>
                </table>
              </td>
            <tr>
          </tbody>
        </table>
      </td>
    </tr>
    <tr>
      <td style="height:5px;line-height:5px;">&nbsp;</td>
    </tr>
    <tr>
      <td>
        #parse( $footer )
      </td>
    <tr>
  </tbody>
</table>
