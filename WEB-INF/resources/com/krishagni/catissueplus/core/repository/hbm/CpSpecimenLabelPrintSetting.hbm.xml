<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.biospecimen.domain.CpSpecimenLabelPrintSetting"
    table="OS_CP_SPMN_LBL_PRINT_SETTINGS">
	
    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_SPMN_LBL_PRINT_SETTINGS_SEQ</param>
      </generator>
    </id>
    
    <many-to-one name="collectionProtocol" column="CP_ID"/>
    
    <property name="lineage" column="LINEAGE"/>
    
    <property name="printMode" column="PRINT_MODE">
      <type name="org.hibernate.type.EnumType">
        <param name="enumClass">
          com.krishagni.catissueplus.core.biospecimen.domain.CollectionProtocol$SpecimenLabelAutoPrintMode
        </param>
        <param name="type">12</param>
      </type>
    </property>
    
    <property name="copies" column="COPIES"/>
  </class>
</hibernate-mapping>
