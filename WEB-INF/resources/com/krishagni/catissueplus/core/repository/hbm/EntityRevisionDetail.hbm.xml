<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.audit.domain.EntityRevisionDetail" table="OS_ENTITY_REVISION_DETAILS">

    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_ENTITY_REV_DETAIL_SEQ</param>
      </generator>
    </id>
    
    <many-to-one name="revision" column="REV"/>

    <property name="type" column="REV_TYPE"/>

    <property name="entityName" column="ENTITY_NAME"/>

    <property name="entityId" column="ENTITY_ID"/>
  </class>
</hibernate-mapping>