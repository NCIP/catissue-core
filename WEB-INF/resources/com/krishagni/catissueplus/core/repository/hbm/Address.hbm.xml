<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping auto-import="false">
    <class
        name="com.krishagni.catissueplus.core.administrative.domain.Address"
        table="CATISSUE_ADDRESS"
	lazy="false" >
    
        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null">

            <generator class="native">
                <param name="sequence">CATISSUE_ADDRESS_SEQ</param>
            </generator>
        </id>

        <property
            name="street"
            type="string"
            update="true"
            insert="true"
            column="STREET"
            length="50"/>

        <property
            name="city"
            type="string"
            update="true"
            insert="true"
            column="CITY"
            length="50"/>

        <property
            name="state"
            type="string"
            update="true"
            insert="true"
            column="STATE"
            length="50"/>

        <property
            name="country"
            type="string"
            update="true"
            insert="true"
            column="COUNTRY"
            length="50"/>

        <property
            name="zipCode"
            type="string"
            update="true"
            insert="true"
            column="ZIPCODE"
            length="30"/>

        <property
            name="phoneNumber"
            type="string"
            update="true"
            insert="true"
            column="PHONE_NUMBER"
            length="50"/>

        <property
            name="faxNumber"
            type="string"
            update="true"
            insert="true"
            column="FAX_NUMBER"
            length="50"/>
            
    </class>
</hibernate-mapping>
