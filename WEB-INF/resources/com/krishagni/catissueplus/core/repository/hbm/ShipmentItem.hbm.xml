<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.administrative.domain.ShipmentItem" 
    table="OS_SHIPMENT_ITEMS">
    
    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_SHIPMENT_ITEMS_SEQ</param>
      </generator>
    </id>
  
    <many-to-one name="shipment" column="SHIPMENT_ID"/>
      
    <many-to-one name="specimen" column="SPECIMEN_ID"/>
      
    <property name="receivedQuality" column="RECEIVED_QUALITY">
      <type name="org.hibernate.type.EnumType">
        <param name="enumClass">com.krishagni.catissueplus.core.administrative.domain.ShipmentItem$ReceivedQuality</param>
        <param name="type">12</param>
      </type>
    </property>
    
  </class>
</hibernate-mapping>
